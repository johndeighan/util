         "# var-free.lib.civet\n\n# ---------------------------------------------------------------------------\n\nexport mapper := <TIn, TOut>(\n\t\tlItems: Iterable<TIn>\n\t\tfunc: (item: TIn) => Generator<TOut, void, void>\n\t\tabort_func: (item: TIn) => boolean = (item: TIn) => return false\n\t\t): Generator<TOut, void, void> ->\n\n\tfor item of lItems\n\t\tif abort_func(item)\n\t\t\treturn\n\t\tyield* func(item)\n\treturn\n\n# ---------------------------------------------------------------------------\n\nexport reducer := <TIn, TAccum>(\n\t\tlItems: Iterable<TIn>\n\t\taccum: TAccum\n\t\treduce_func: (accum: TAccum, item: TIn) => TAccum\n\t\tabort_func: (item: TIn) => boolean = (item: TIn) => return false\n\t\t): TAccum ->\n\n\tfor item of lItems\n\t\tif abort_func item\n\t\t\tbreak\n\t\taccum = reduce_func(accum, item)\n\treturn accum\n"
