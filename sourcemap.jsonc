         "# var-free.lib.test.civet\n\nimport {mapper, reducer } from 'var-free'\nimport {equal} from 'unit-test'\n\n# ---------------------------------------------------------------------------\n\n(() =>\n\t# --- pure mapping\n\n\tlItems := [1, 2, 3, 4]\n\tlNewItems := mapper lItems, (i: number) ->\n\t\tyield \"number is #{i}\"\n\n\tequal lNewItems,\n\t\t[\n\t\t\t\"number is 1\"\n\t\t\t\"number is 2\"\n\t\t\t\"number is 3\"\n\t\t\t\"number is 4\"\n\t\t\t]\n\t)()\n\n(() =>\n\t# --- pure mapping, early abort\n\n\tlItems := [1, 2, 3, 4]\n\tfunc := (i: number) ->\n\t\tyield \"number is #{i}\"\n\t\tif (i == 2)\n\t\t\treturn\n\tabort_func := (i: number) ->\n\t\treturn (i == 3)\n\tlNewItems := mapper lItems, func, abort_func\n\n\tequal lNewItems,\n\t\t[\n\t\t\t\"number is 1\"\n\t\t\t\"number is 2\"\n\t\t\t]\n\t)()\n\n(() =>\n\t# --- pure mapping, using a generator\n\n\tgen := (): Generator<number, void, void> ->\n\t\tyield 1\n\t\tyield 2\n\t\tyield 3\n\t\tyield 4\n\n\tlNewItems := mapper gen(), (i: number) ->\n\t\tyield \"number is #{i}\"\n\n\tequal lNewItems,\n\t\t[\n\t\t\t\"number is 1\"\n\t\t\t\"number is 2\"\n\t\t\t\"number is 3\"\n\t\t\t\"number is 4\"\n\t\t\t]\n\t)()\n\n(() =>\n\t# --- pure filtering\n\n\tlItems := [1, 2, 3, 4]\n\tlNewItems := mapper lItems, (i: number) ->\n\t\tif (i % 2 == 0)\n\t\t\tyield i\n\n\tequal lNewItems, [2, 4]\n\t)()\n\n(() =>\n\t# --- pure filtering, early abort\n\n\tlItems := [1, 2, 3, 4]\n\tfunc := (i: number) ->\n\t\tif (i % 2 == 0)\n\t\t\tyield i\n\tabort_func := (i: number) ->\n\t\treturn (i == 3)\n\tlNewItems := mapper lItems, func, abort_func\n\n\tequal lNewItems, [2]\n\t)()\n\n(() =>\n\t# --- combined filtering and mapping\n\n\tlItems := [1, 2, 3, 4]\n\tlNewItems := mapper lItems, (i: number) ->\n\t\tif (i % 2 == 0)\n\t\t\tyield \"2i = #{2*i}\"\n\t\t\tyield \"number is #{10 * i}\"\n\t\treturn\n\n\tequal lNewItems,\n\t\t[\n\t\t\t\"2i = 4\"\n\t\t\t\"number is 20\"\n\t\t\t\"2i = 8\"\n\t\t\t\"number is 40\"\n\t\t\t]\n\t)()\n\n(() =>\n\t# --- reducing (getting the sum, use JavaScript's reduce)\n\n\tlItems := [1, 2, 3, 4]\n\tfunc := (accum: number, n: number) =>\n\t\treturn accum + n\n\tsum := lItems.reduce func, 0\n\tequal sum, 10\n\t)()\n\n(() =>\n\t# --- reducing (getting the sum)\n\n\tlItems := [1, 2, 3, 4]\n\tfunc := (accum: number, n: number) =>\n\t\treturn accum + n\n\tsum := reducer lItems, 0, func\n\tequal sum, 10\n\t)()\n\n(() =>\n\t# --- reducing (getting the sum, early abort)\n\n\tlItems := [1, 2, 3, 4]\n\tfunc := (accum: number, n: number) =>\n\t\treturn accum + n\n\tabort_func := (i: number) ->\n\t\treturn (i == 3)\n\tsum := reducer lItems, 0, func, abort_func\n\tequal sum, 3\n\t)()\n\n(() =>\n\t# --- reducing (getting the sum AND sum of squares)\n\n\tlItems := [1, 2, 3, 4]\n\ttype acc = [number, number]\n\tfunc := (accum: acc, n: number): acc =>\n\t\t[sum, sumsq] := accum\n\t\treturn [sum + n, sumsq + n*n]\n\n\t[sum, sumsq] := reducer lItems, [0, 0], func\n\tequal sum, 10\n\tequal sumsq, 30\n\t)()\n\n(() =>\n\t# --- reducing\n\t#     (getting the sum AND sum of squares of only even nums)\n\n\tlItems := [1, 2, 3, 4]\n\ttype acc = [number, number]\n\tfunc := (accum: acc, n: number): acc =>\n\t\tif (n % 2 == 0)\n\t\t\t[sum, sumsq] := accum\n\t\t\treturn [sum + n, sumsq + n*n]\n\t\telse\n\t\t\treturn accum\n\n\t[sum, sumsq] := reducer lItems, [0, 0], func\n\tequal sum, 6\n\tequal sumsq, 20\n\t)()\n"
