         "# hera-compile.lib.test.civet\n\nimport {esc, mesc} from 'unicode'\nimport {\n\tdoCompileHera, compileHera,\n\t} from 'hera-compile'\nimport {\n\tequal, truthy, falsy, succeeds, fails,\n\t} from 'unit-test'\n\n# ---------------------------------------------------------------------------\n\n# --- Very simple rule\n\nequal esc(compileHera(\"\"\"\n\tMain\n\t\t/abc/\n\t\"\"\")), 'Main/abc/'\n\n# --- Very simple rule, with comment and blank like\n\nequal mesc(compileHera(\"\"\"\n\t# file\n\n\tMain\n\t\t/abc/\n\t\"\"\")), \"\"\"\n\t#file\n\t\n\tMain\n\t/abc/\n\t\"\"\"\n\n# --- Some code to execute\n\nequal mesc(compileHera(\"\"\"\n\tMain\n\t\t/abc/ ->\n\t\t\twrite\n\t\"\"\")), \"\"\"\n\tMain\n\t/abc/->\n\twrite\n\t\"\"\"\n\n# --- This should fail, '->' is missing\n\nfails () => compileHera(\"\"\"\n\tMain\n\t\t/abc/\n\t\t\twrite\n\t\"\"\")\n\n# --- add a code block\n\nequal mesc(compileHera(\"\"\"\n\t# --- my parser\n\n\t```\n\t\tconsole.log('Hello');\n\t```\n\n\tMain\n\t\t/abc/ ->\n\t\t\twrite\n\t\"\"\")), \"\"\"\n\t#---myparser\n\t\n\t```\n\tconsole.log('Hello');\n\t```\n\t\n\tMain\n\t/abc/->\n\twrite\n\t\"\"\"\n\n# --- add a multiple code blocks\n\nequal mesc(compileHera(\"\"\"\n\t# --- my parser\n\n\t```\n\t\tconsole.log('Hello');\n\t```\n\n\t```\n\t\tconsole.log('Hello');\n\t```\n\n\tMain\n\t\t/abc/ ->\n\t\t\twrite\n\t\"\"\")), \"\"\"\n\t#---myparser\n\t\n\t```\n\tconsole.log('Hello');\n\t```\n\t\n\t```\n\tconsole.log('Hello');\n\t```\n\t\n\tMain\n\t/abc/->\n\twrite\n\t\"\"\"\n\n# --- Multiple Rules\n\nequal mesc(compileHera(\"\"\"\n\tMain\n\t\tRoot \"abc\" ->\n\t\t\twrite $2\n\n\tRoot\n\t\t/\\..*/ ->\n\t\t\twrite $1\n\t\t\texit\n\t\"\"\")), \"\"\"\n\tMain\n\tRoot\"abc\"->\n\twrite$2\n\t\n\tRoot\n\t/\\..*/->\n\twrite$1\n\texit\n\t\"\"\"\n"
