         "# call-stack.lib.civet\n\nimport {CallSite} from 'npm:@types/node'\nimport {croak} from 'datatypes'\nimport {relpath} from 'fsys'\nimport {getErrStr} from 'exec'\nimport {TFilePos, mapSourcePos} from 'source-map'\n\n# ---------------------------------------------------------------------------\n\nexport type TStackFrame = {\n\tsource: string\n\tline: number\n\tcol: number\n\t}\n\n# ---------------------------------------------------------------------------\n\nexport getCallSites := (): CallSite[] =>\n\n\tdebugger\n\ttry\n\t\t# @ts-ignore\n\t\toldLimit := Error.stackTraceLimit\n\t\t# @ts-ignore\n\t\toldPreparer := Error.prepareStackTrace\n\t\t# @ts-ignore\n\n\t\tError.stackTraceLimit = Infinity\n\n\t\t# @ts-ignore\n\t\tError.prepareStackTrace = (error, lCallSites) =>\n\t\t\treturn lCallSites\n\n\t\t# --- Accessing err.stack calls the custom function\n\t\t#     and returns the array of CallSite objects\n\t\t# --- Create an error to trigger the custom prepareStackTrace\n\t\terr := new Error()\n\n\t\t# @ts-ignore\n\t\tlCallSites := err.stack\n\n\t\t# --- reset to previous values\n\t\t# @ts-ignore\n\t\tError.stackTraceLimit = oldLimit\n\t\t# @ts-ignore\n\t\tError.prepareStackTrace = oldPreparer\n\t\t# @ts-ignore\n\t\treturn lCallSites\n\n\tcatch err\n\t\tconsole.log \"ERROR: #{getErrStr(err)}\"\n\t\treturn []\n\n# ---------------------------------------------------------------------------\n\nexport getStack := (): TStackFrame[] =>\n\n\tlFrames := for frame of getCallSites()\n\t\tsource := relpath(frame.getFileName()).replace('src/lib/src/lib/', 'src/lib/').replace('src/temp/src/temp/', 'src/temp/')\n\t\tline := frame.getLineNumber()\n\t\tcol := frame.getColumnNumber()\n\n\t\thNew := mapSourcePos {\n\t\t\tsource\n\t\t\tline\n\t\t\tcolumn: col\n\t\t\t}\n\n\t\t{\n\t\t\tsource: hNew.source\n\t\t\tline: hNew.line\n\t\t\tcol: hNew.column\n\t\t\t}\n\treturn lFrames.filter (frame) => not frame.source.includes('call-stack.lib')\n\n"
