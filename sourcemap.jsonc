'gender')\n\n\twalker := new NewPersonWalker()\n\n\tlDesc := for {name, gender} of walker.walk(hAst)\n\t\t\"#{name} is #{gender}\"\n\n\tequal lDesc, [\n\t\t'John is male'\n\t\t'Julie is female'\n\t\t'Bob is male'\n\t\t'Jane is female'\n\t\t]\n\n\t# --- Try extended walking\n\n\tlDesc2 := for [vkind, {name, gender}] of walker.walkEx(hAst)\n\t\twhen (vkind == 'enter')\n\t\t\"#{name} is #{gender}\"\n\n\tequal lDesc2, [\n\t\t'John is male'\n\t\t'Julie is female'\n\t\t'Bob is male'\n\t\t'Jane is female'\n\t\t]\n\t)()\n\n# ---------------------------------------------------------------------------\n# --- test extended walking\n\n(() =>\n\thAst := {\n\t\tkind: 'top'\n\t\tfirst: 'first'\n\t\tnext: {\n\t\t\tkind: 'next'\n\t\t\ta: 'one'\n\t\t\tb: 'two'\n\t\t\tc: 'three'\n\t\t\t}\n\t\tlast: {\n\t\t\tkind: 'last'\n\t\t\tA: 'one'\n\t\t\tB: 'two'\n\t\t\t}\n\t\t}\n\n\n\twalker := new Walker()\n\tlItems := for [vkind, item] of walker.walkEx(hAst)\n\t\t\"#{vkind} #{item.kind}\"\n\tequal lItems, [\n\t\t'enter top'\n\t\t'enter next'\n\t\t'exit next'\n\t\t'enter last'\n\t\t'exit last'\n\t\t'exit top'\n\t\t]\n\t)()\n"
