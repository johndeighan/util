         "# console-utils.lib.civet\n\n# ---------------------------------------------------------------------------\n\nencoder := new TextEncoder()\n\nexport type TLogLevel = 'silent' | 'info' | 'debug'\n\nlet logLevel: TLogLevel = 'info'\n\n# --- useful for unit testing\n#     or saving final log to a log file\nexport let logStr = ''\n\n# ---------------------------------------------------------------------------\n\nexport setLogLevel := (level: TLogLevel): void =>\n\n\tlogLevel = level\n\treturn\n\n# ---------------------------------------------------------------------------\n\nexport LOG := (str: string): void =>\n\n\tif (logLevel != 'silent')\n\t\twriteln str\n\treturn\n\n# ---------------------------------------------------------------------------\n\nexport DBG := (str: string): void =>\n\n\tif (logLevel == 'debug')\n\t\twriteln str\n\treturn\n\n# ---------------------------------------------------------------------------\n\nexport write := (str: string): void =>\n\n\tDeno.stdout.writeSync encoder.encode(str)\n\tlogStr += str\n\treturn\n\n# ---------------------------------------------------------------------------\n\nexport writeln := (str: string = ''): void =>\n\n\twrite str + '\\n'\n\treturn\n\n# ---------------------------------------------------------------------------\n\nexport clearScreen := (): void =>\n\n\twrite '\\x1b[H\\x1b[2J'\n\tlogStr += '\\n'\n\treturn\n\n# ---------------------------------------------------------------------------\n\nexport resetLine := (): void =>\n\n\twrite '\\r'\n\twrite \"\\x1b[K\"\n\treturn\n"
