         "# parse-utils.lib.civet\n\nimport {sprintf} from 'jsr:@std/fmt/printf'\nimport {uni, esc} from 'unicode'\nimport {undef, defined, assert, croak, hash, hashof} from 'datatypes'\nimport {range, centered, f} from 'llutils'\nimport {OL} from 'to-nice'\nimport {TextTable} from 'text-table'\n\n# ---------------------------------------------------------------------------\n\nexport slot := (name: string, width: number): [string, string] =>\n\n\tswitch width\n\t\tcase 0:\n\t\t\treturn ['', '']\n\t\tcase 1:\n\t\t\treturn [\n\t\t\t\tuni.vbar\n\t\t\t\tname[0]\n\t\t\t]\n\t\tcase 2:\n\t\t\treturn [\n\t\t\t\tuni.blcorner + uni.brcorner\n\t\t\t\tname.substring(0, 2)\n\t\t\t]\n\t\tdefault:\n\t\t\treturn [\n\t\t\t\tuni.blcorner + uni.hbar.repeat(width - 2) + uni.brcorner\n\t\t\t\tcentered(name.substring(0, width), ' ', width)\n\t\t\t]\n\n# ---------------------------------------------------------------------------\n\nexport type TStrLoc =\n\tpos: number\n\tlength: number\n\nexport type TParseMatch = [name: string, pos: number, len: number]\n\n# ---------------------------------------------------------------------------\n\nexport class CParseMatches\n\tlParseMatches: TParseMatch[] = []\n\n\t# ----------------------------------------------------------\n\n\tmatch(name: string, loc: TStrLoc | [number, number]): void\n\n\t\tassert defined(@lParseMatches), \"undef lParseMatches\"\n\t\tif Array.isArray(loc)\n\t\t\t@lParseMatches.push [name, ...loc]\n\t\telse\n\t\t\t@lParseMatches.push [name, loc.pos, loc.length]\n\t\treturn\n\n\t# ----------------------------------------------------------\n\n\tmatchesStr(): string\n\n\t\tresults := []\n\t\tfor [name, pos, len] of @lParseMatches\n\t\t\tresults.push f\"#{pos}:15 #{len}:3 #{name}:15\"\n\t\tlLines := results\n\t\treturn lLines.join '\\n'\n\n\t# ----------------------------------------------------------\n\n\tmatchesTable(): string\n\n\t\tresults := []\n\t\ttable := new TextTable \"l r%d r%d l\"\n\t\ttable.labels ['Op', 'Pos', 'Len', 'Data']\n\t\tfor [name, pos, len] of @lParseMatches\n\t\t\ttable.data [name, pos, len, '']\n\t\treturn table.asString()\n\n\t# ----------------------------------------------------------\n\n\tdebugStr(\n\t\t\tstr: string,\n\t\t\thReplace: hashof<string> = {}\n\t\t\t): string\n\n\t\tn := @lParseMatches.length\n\t\tdebugger\n\t\tlLineNum: number[] := new Array<number>(n).fill 0\n\t\tlLines: string[] := []\n\t\tlet nRemaining = n\n\t\tlet lineNum = 0\n\t\twhile nRemaining > 0\n\t\t\tlet line1 = ''\n\t\t\tlet line2 = ''\n\t\t\tlineNum += 1\n\t\t\tlet linePos = 0\n\t\t\tlet i1 = 0\n\t\t\tfor [name, pos, len] of @lParseMatches\n\t\t\t\ti := i1++\n\t\t\t\tif (pos >= linePos) && (lLineNum[i] == 0)\n\t\t\t\t\tline1 += ' '.repeat(pos - linePos)\n\t\t\t\t\tline2 += ' '.repeat(pos - linePos)\n\t\t\t\t\t[str1, str2] := slot hReplace[name] || name, len\n\t\t\t\t\tline1 += str1\n\t\t\t\t\tline2 += str2\n\t\t\t\t\tlLineNum[i] = lineNum\n\t\t\t\t\tnRemaining -= 1\n\t\t\t\t\tlinePos = pos + len\n\t\t\tlLines.push line1, line2\n\t\treturn esc(str) + '\\n' + lLines.join('\\n')\n\n"
